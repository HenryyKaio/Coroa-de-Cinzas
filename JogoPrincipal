programa
{
	inclua biblioteca Util --> u
	inclua biblioteca Tipos --> t
	inclua biblioteca Texto --> tx

	//Variáveis globais
	logico gameLoop = verdadeiro
	cadeia itens[6] = {"vazio", "vazio", "vazio", "vazio", "vazio", "vazio"}
	inteiro vida = 100
	inteiro dinheiro = 345
	inteiro vidaUbariano2= 200
	inteiro danoInimigo = 15
	inteiro dano = u.sorteia(10,20)
	inteiro vidaInimigo = 70
	cadeia nome
	
	
	// Logicos Quarto Donzela
	logico barro = falso
	logico petala = falso

	// Logico para ver se estava ou não com a faca
	logico Faca = falso
	
	// Logicos Perola Vermelha
	logico masmorra = falso
	logico investigacao = falso
	logico livro = falso
	
	// logicos Caso 1
	logico bruxa = falso
	logico doente = falso
	
	logico pocaoForca = falso
	logico pocaoCura = falso
	logico pocaoResistencia = falso

	logico LutaUbariano = falso

	// Logicos caso 2
	logico florestaCs2 = falso
	
	funcao inicio()
	{	


		Introducao()
		PrimeiraParte()
		QuartoDonzela()
		Jardim()
		JardimCozinha()
		PerolaVermelha()

		se(livro == verdadeiro){
			
			LivrosCs1()
			DiarioCs1()
			CasaCs1() 
			FlorestaSangrentaCs1()
			
			se(LutaUbariano == verdadeiro){
			se(gameLoop == verdadeiro){
					LutaCs(1)
					FinalCs1()	
			}

					
			}
		}
		se(masmorra == verdadeiro){
			MasmorraCs2()
			SaindoMasmorraCs2()
			se(florestaCs2 == verdadeiro){
			se(gameLoop == verdadeiro){
			florestaSangrentaCs2()
			se(gameLoop == verdadeiro){
			LutaCs(2)
			FinalCs2()	
			}

			}

			
			}
		}

		se(investigacao == verdadeiro){
			investigacaoCs3()
			florestaSangrentaCs3()
			LutaCs(3)
			FinalCs3()
		}
		


		
	
	}

	

