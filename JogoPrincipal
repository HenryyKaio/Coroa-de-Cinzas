	funcao DiarioCs1(){
		escreve("Você começa a vasculhar outros itens da estante. \n")
		escreve("Tem diversas deorações e livros diferentes. \n")
		escreve("Você vê um livro deitado, atras de outros na estante. \n")
		escreve("Do lado uma caixinha de madeira com um simbolo de caveira. \n")
		escreve("Qual item vasculhar? \n")

		logico continuar = verdadeiro
		enquanto(continuar == verdadeiro){
			
			escreve("[1] - Livro escondido.\n")
			escreve("[2] - Caixa misteriosa. \n")
			inteiro opcao
			leia(opcao)

			escolha(opcao){
