	funcao pagina110(){
		escreva("╭────────────────────────────────────────────╮", "\n")
		escreva("│                                            │", "\n")
		escreva("│  Querido diário,                           │", "\n")
		escreva("│  não sei mais o que fazer,                 │", "\n")
		escreva("│  minhas mudanças de humor estão            │", "\n")
		escreva("│  me matando. Não tenho apetite             │", "\n")
		escreva("│  para nada, isso esta me preocupando.      │", "\n")
		escreva("│  Vou ir para a floresta sangrenta          │", "\n")
		escreva("│  procurar a erva Tocrunda, os livros       │", "\n")
		escreva("│  dizem que cura tudo. Deve curar tudo      │", "\n")
		escreva("│  isso que estou sentindo também.           │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│  Página 110                                │", "\n")
		escreva("│                                            │", "\n")
		escreva("╰────────────────────────────────────────────╯", "\n")
		enterCarta()
	}
	
	funcao pagina111(){
		escreva("╭────────────────────────────────────────────╮", "\n")
		escreva("│                                            │", "\n")
		escreva("│  Querido diário,                           │", "\n")
		escreva("│  os testes deram positivo.                 │", "\n")
		escreva("│  Estou com medo da minha                   │", "\n")
		escreva("│  transformação acontecer a qualquer        │", "\n")
		escreva("│  momento. Vou procurar respostas           │", "\n")
		escreva("│  com uma bruxa na floresta sangrenta,      │", "\n")
		escreva("│  é a minha única chance.                   │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│  Página 111                                │", "\n")
		escreva("│                                            │", "\n")
		escreva("╰────────────────────────────────────────────╯", "\n")
		enterCarta()
	}
	
	funcao pedacosCarta(){
		
		escreva("╭────────────╮", "\n")
		escreva("│ Querido di │", "\n")
		escreva("│ ...        │", "\n")
		escreva("╰────────────╯", "\n")
		escreva("\n")

		escreva("╭────────────────────╮", "\n")
		escreva("│ ...s deram posit...│", "\n")
		escreva("│ Estou com medo...  │", "\n")
		escreva("╰────────────────────╯", "\n")
		escreva("\n")

		escreva("╭────────────────────────────╮", "\n")
		escreva("│ ...u procurar  ...         │", "\n")
		escreva("│ com uma bruxa na floresta  │", "\n")
		escreva("│ sangrenta...               │", "\n")
		escreva("╰────────────────────────────╯", "\n")
		enterCarta()

	}
	
	funcao flor(){
		escreva("\n")
		escreva("╭────────────────────────────────────────────╮", "\n")
		escreva("│                                            │", "\n")
		escreva("│            /\\^/`\\                          │", "\n")
		escreva("│           | \\/   ||                        │", "\n")
		escreva("│           | |    ||                        │", "\n")
		escreva("│            \\ \\   /                         │", "\n")
		escreva("│             '\\\\//'                         │", "\n")
		escreva("│               ||                           │", "\n")
		escreva("│               ||                           │", "\n")
		escreva("│               ||                           │", "\n")
		escreva("│               ||                           │", "\n")
		escreva("│          |\\   ||  |\\                       │", "\n")
		escreva("│           | | ||  | |                      │", "\n")
		escreva("│           | | || / /                       │", "\n")
		escreva("│           \\  \\||/ /                        │", "\n")
		escreva("│                                            │", "\n")
		escreva("╰────────────────────────────────────────────╯", "\n")
	}
	
	funcao castelo(){
		
		escreva("                                                  |*~=-.,   \n")
		escreva("                                                  |_,-'`    \n")
		escreva("                                                  |         \n")
		escreva("                                                  |         \n")
		escreva("                                                 /^\\        \n")
		escreva("                   !_                           /   \\       \n")
		escreva("                   |*`~-.,                     /,    \\      \n")
		escreva("                   |.-~^`                     /#\"     \\     \n")
		escreva("                   |                        _/##_   _  \\_   \n")
		escreva("              _   _|  _   _   _            [ ]_[ ]_[ ]_[ ]  \n")
		escreva("             [ ]_[ ]_[ ]_[ ]_[ ]            |_=_-=_ - =_|   \n")
		escreva("           !_ |_=_ =-_-_  = =_|           !_ |=_= -    |    \n")
		escreva("           |*`--,_- _        |            |*`~-.,= []  |    \n")
		escreva("           |.-'|=     []     |   !_       |_.-\"`_-     |    \n")
		escreva("           |   |_=- -        |   |*`~-.,  |  |=_-      |    \n")
		escreva("          /^\\  |=_= -        |   |_,-~`  /^\\ |_ - =[]  |    \n")
		escreva("      _  /   \\_|_=- _   _   _|  _|  _   /   \\|=_-      |    \n")
		escreva("     [ ]/,    \\[ ]_[ ]_[ ]_[ ]_[ ]_[ ]_/,    \\[ ]=-    |    \n")
		escreva("      |/#\"     \\_=-___=__=__- =-_ -=_ /#\"     \\| _ []  |    \n")
		escreva("     _/##_   _  \\_-_ =  _____       _/##_   _  \\_ -    |\\   \n")
		escreva("    [ ]_[ ]_[ ]_[ ]=_0~{_ _ _}~0   [ ]_[ ]_[ ]_[ ]=-   | \\  \n")
		escreva("    |=__-_=-_  =_|-=_  |     |     |_=-___-_ =-__|_    |  \\ \n")
		escreva("     | _- =-     |-_   |     |      |= _=       | -    |___\\ \n")
		escreva("     |= -_=      |=  _ |     |      |_-=_       |=_    |/+\\|\n")
		escreva("     | =_  -     |_ = _ `-.-`       | =_ = =    |=_-   ||+||\n")
		escreva("     |-_=- _     |=_   =            |=_= -_     |  =   ||+||\n")
		escreva("     |=_- /+\\    | -=               |_=- /+\\    |=_    |^^^|\n")
		escreva("     |=_ |+|+|   |= -  -_,--,_      |_= |+|+|   |  -_  |=  |\n")
		escreva("     |  -|+|+|   |-_=  / |  | \\     |=_ |+|+|   |-=_   |_-/ \n")
		escreva("     |=_=|+|+|   | =_= | |  | |     |_- |+|+|   |_ =   |=/  \n")
		escreva("     | _ ^^^^^   |= -  | |  <&>     |=_=^^^^^   |_=-   |/   \n")
		escreva("     |=_ =       | =_-_| |  | |     |   =_      | -_   |    \n")
		escreva("     |_=-_       |=_=  | |  | |     |=_=        |=-    |    \n")
	}

		funcao Ubariano(){
		
		escreva("                        _.-.", "\n")
		escreva("                       ._.-.\\", "\n")
		escreva("                     _.-'=. \\\\", "\n")
		escreva("                 .-._.-=-..' \\'.", "\n")
		escreva("               _.--._-='.'   |  `.", "\n")
		escreva("              _`.-.`=-./'.-. / .-.\\", "\n")
		escreva("              _.-=-=-/ | '._)`(_.'|", "\n")
		escreva("              _.--=.'  `. (.-v-.)/", "\n")
		escreva("              _.-.' \\-.' `-..-..'", "\n")
		escreva("             .-.'      `.'  \" \".", "\n")
		escreva("            /   )        \\  /   \\", "\n")
		escreva("           /   /\\                \\", "\n")
		escreva("         .'  .'\\ `.        .'     \\", "\n")
		escreva("        /   /   \\  `-    -' .`.    .", "\n")
		escreva("      .''\\.'    | `.      .'   `.   \\", "\n")
		escreva("     /   |      |      .'/       `.- `.", "\n")
		escreva("    |   /       / `.    |         \\   |", "\n")
		escreva("    |  /       |       .'.         `. |", "\n")
		escreva("    / /       /           \\          \\ \\", "\n")
		escreva("   /  |      /             \\         |  `.", "\n")
		escreva("  /   `.     |      `   .'  \\        /    \\", "\n")
		escreva(" ///.-'.\\    |       \\ /    `\\      / /-.  \\", "\n")
		escreva(" \\\\\\   `     \\.-     |    `-.\\     '/   \\\\\\", "\n")
		escreva("  \\\\|        |      / \\      |          ////", "\n")
		escreva("   '''        |     /   \\     |          |//", "\n")
		escreva("              |.-  |     \\  .-|          ''", "\n")
		escreva("              /    |    / ` ../", "\n")
		escreva("              |'   /    |    /", "\n")
		escreva("              \\\\.'  |    | `./", "\n")
		escreva("              /    \\   /    \\", "\n")
		escreva("              \\ `. /   \\    /", "\n")
		escreva("               |  |     '. '", "\n")
		escreva("               /  |      |  \\", "\n")
		escreva("              /   |      /   `.", "\n")
		escreva("             | | | \\   .'  `.. \\", "\n")
		escreva("            / / / ._`. \\\\.'-. \\`/", "\n")
		escreva("            |/ / /  `'  `  |/|/", "\n")
		escreva("             \\\\|\\|", "\n")

	}

	funcao Ubariano2(){
		escreva("                                  _.-.", "\n")
		escreva("                                 ._.-.\\", "\n")
		escreva("                    .^         _.-'=. \\\\", "\n")
		escreva("                  .'  )    .-._.-=-..' \\'.", "\n")
		escreva("               .'   .'   _.--._-='.'   |  `.  ^.", "\n")
		escreva("             .'   .'    _`.-.`=-./'.-. / .-.\\ `. `.", "\n")
		escreva("           .'    /      _.-=-=-/ | '._)`(_.'|   \\  `.", "\n")
		escreva("          /    /|       _.--=.'  `. (.-v-.)/    |\\   \\", "\n")
		escreva("        .'    / \\       _.-.' \\-.' `-..-..'     / \\   `.", "\n")
		escreva("       /     /   `-.._ .-.'      `.'  \" \". _..-'  |    |", "\n")
		escreva("      '      |    |   /   )        \\  /   \\   \\    \\    `.", "\n")
		escreva("     /      /    /   /   /\\                \\   \\   |      \\", "\n")
		escreva("    /      /    /  .'  .'\\ `.        .'     \\   |   \\      \\", "\n")
		escreva("   /      /    /  /   /   \\  `-    -' .`.    .  \\    \\     |", "\n")
		escreva("  |      /    / .''\\.'    | `.      .'   `.   \\  |    |    |", "\n")
		escreva(" .'     /    / /   |      |      .'/       `.- `./    /    |", "\n")
		escreva(" |     /    .-|   /--.    / `.    |    _.-''\\   |     |    \\", "\n")
		escreva(".'    /  .-'  |  /    `-.|       .'\\_.'      `. |`.   |    |", "\n")
		escreva("|    |.-'     / /       /           \\          \\ \\ `. \\     \\", "\n")
		escreva("|    /       /  |      /             \\         |  `. `.|    |", "\n")
		escreva("|   |       /   `.     |      `   .'  \\        /    \\  \\    /", "\n")
		escreva("|   |      ///.-'.\\    |       \\ /    `\\      / /-.  \\ |    |", "\n")
		escreva("|   /      \\\\\\\\    `    \\.-     |    `-.\\     |/   \\\\\\\\'.   |", "\n")
		escreva(" \\ |        \\\\\\|        |      / \\      |          //// |  /", "\n")
		escreva(" | |         '''        |     /   \\     |          |//  |  \\", "\n")
		escreva(" \\ |                    |.-  |     \\  .-|          ''   |  /", "\n")
		escreva("  \\|                    /    |    / ` ../               / /", "\n")
		escreva("                        |'   /    |    /               | /", "\n")
		escreva("                        \\\\.'  |    | `./                |/", "\n")
		escreva("                        /    \\   /    \\", "\n")
		escreva("                        \\ `. /   \\    /", "\n")
		escreva("                         |  |     '. '", "\n")
		escreva("                         /  |      |  \\", "\n")
		escreva("                        /   |      /   `.", "\n")
		escreva("                       | | | \\   .'  `.. \\", "\n")
		escreva("                      / / / ._`. \\\\.'-. \\`/", "\n")
		escreva("                      |/ / /  `'  `  |/|/", "\n")
		escreva("                       \\\\|\\|", "\n")

}


	
	funcao GameOver(){
		
		escreva("  ▄████  ▄▄▄       ███▄ ▄███▓▓█████     ▒█████   ██▒   █▓▓█████  ██▀███  ", "\n")
		u.aguarde(200)
		escreva(" ██▒ ▀█▒▒████▄    ▓██▒▀█▀ ██▒▓█   ▀    ▒██▒  ██▒▓██░   █▒▓█   ▀ ▓██ ▒ ██▒", "\n")
		u.aguarde(200)
		escreva("▒██░▄▄▄░▒██  ▀█▄  ▓██    ▓██░▒███      ▒██░  ██▒ ▓██  █▒░▒███   ▓██ ░▄█ ▒", "\n")
		u.aguarde(200)
		escreva("░▓█  ██▓░██▄▄▄▄██ ▒██    ▒██ ▒▓█  ▄    ▒██   ██░  ▒██ █░░▒▓█  ▄ ▒██▀▀█▄  ", "\n")
		u.aguarde(200)
		escreva("░▒▓███▀▒ ▓█   ▓██▒▒██▒   ░██▒░▒████▒   ░ ████▓▒░   ▒▀█░  ░▒████▒░██▓ ▒██▒", "\n")
		u.aguarde(200)
		escreva(" ░▒   ▒  ▒▒   ▓▒█░░ ▒░   ░  ░░░ ▒░ ░   ░ ▒░▒░▒░    ░ ▐░  ░░ ▒░ ░░ ▒▓ ░▒▓░", "\n")
		u.aguarde(200)
		escreva("  ░   ░   ▒   ▒▒ ░░  ░      ░ ░ ░  ░     ░ ▒ ▒░    ░ ░░   ░ ░  ░  ░▒ ░ ▒░", "\n")
		u.aguarde(200)
		escreva("░ ░   ░   ░   ▒   ░      ░      ░      ░ ░ ░ ▒       ░░     ░     ░░   ░ ", "\n")
		u.aguarde(200)
		escreva("      ░       ░  ░       ░      ░  ░       ░ ░        ░     ░  ░   ░     ", "\n")
		u.aguarde(200)
		escreva("                                                     ░                    ", "\n")
		u.aguarde(200)

	}
	
	funcao EntradaBar(){

		escreva("_______________________________________________________________________________", "\n")
		escreva("                          .-|-|----------------|-|-.", "\n")
		escreva("                          | .-. .-. .  .-. .-. . . |_", "\n")
		escreva("                     .--~~| `-. |-| |  | | | | |/| | ~~--." , "\n")
		escreva("                     |    | `-' ` ' `- `-' `-' ' ' |     |", "\n")
		escreva("                     |    `-.____________________.-'     |", "\n")
		escreva("_______________      |                                   |", "\n")
		escreva("     |         |     |                                   |", "\n")
		escreva("  /  |   /  /  |     |                                   |", "\n")
		escreva("/    |     /   |     | _______                   _______ |", "\n")
		escreva("_____|_________|     ||       ~---_         _---~       ||", "\n")
		escreva("     |    /    |     ||           ~---. .---~           ||", "\n")
		escreva(" / / |         |     ||               | |               ||", "\n")
		escreva("  /  |  /   /  |     ||               | |               ||", "\n")
		escreva("_____|_________|     ||               | |               ||", "\n")
		escreva("---------------'     ||               |  |               ||", "\n")
		escreva("                     ||               | |               ||", "\n")
		escreva("                     ||               | |               ||", "\n")
		escreva("                     ||           .---' `---.           ||", "\n")
		escreva("                     |`.______.---'         `---.______.'|", "\n")
		escreva("               ______|                                   |______", "\n")
		}
	
	funcao bar(){

		escreva("                                                                                          ##########MM                                      \n")
		escreva("                                                                                        @@############                                      \n")
		escreva("                                                                                        ################      ############@@                \n")
		escreva("                                                                                      MM################        ##########++                \n")
		escreva("                                                                                      ##################MM      ####++####                  \n")
		escreva("                                                                                    ######################      ##mm++##..                  \n")
		escreva("                                                                                  ++######################      ##::++##                    \n")
		escreva("                                                                                  ##########################    ##::++++                    \n")
		escreva("                                                                                ..##########################mm  ####++++                    \n")
		escreva("                                                                                ##############################  ####MM::                    \n")
		escreva("                                                                              mm##########++####MM####################++..                  \n")
		escreva("                                                                              ############++mmMM########################++                  \n")
		escreva("                                                                            ############@@@@mm############################                  \n")
		escreva("                                    --      ..                            ##############MM++@@@@##########################                  \n")
		escreva("                              ..##############################@@@@####################@@@@@@@@@@##########################                  \n")
		escreva("                              ##mmmmmm##mmmmmmmm##mmmmmmmm##mmmmmmmm##mm############@@@@@@@@@@@@@@########################                  \n")
		escreva("                            ##@@@@@@@@##@@MM@@####@@@@@@##@@@@MM@@@@##@@############@@@@@@@@@@##@@########################                  \n")
		escreva("                          --##@@@@@@##@@@@@@@@##@@@@@@@@##@@@@@@MM##@@############@@@@@@@@@@######@@################@@####                  \n")
		escreva("                          ##@@@@@@##@@@@@@@@####@@@@@@##@@@@@@@@MM##@@############@@@@@@@@@@######@@##############@@####                    \n")
		escreva("                        ##@@mm@@@@##@@@@@@@@##MM@@@@@@##@@@@@@@@##@@####@@######@@@@@@@@@@@@########@@##########@@########                  \n")
		escreva("                      ::##@@@@MM##@@@@@@@@##@@@@@@@@##@@@@@@@@@@##@@##@@######@@@@@@@@@@@@@@######@@@@########@@##########                  \n")
		escreva("                      ##@@@@@@@@mm@@@@@@####@@@@@@@@##@@@@@@@@##MM##@@########@@@@@@@@@@@@@@######@@@@@@####@@##############                \n")
		escreva("                    ##@@@@@@@@##@@MM@@@@##@@@@@@@@##@@@@@@@@##@@@@##########@@@@##@@##@@@@@@######@@@@@@##@@################                \n")
		escreva("                  ++##@@@@@@@@@@@@@@MM##@@@@@@@@####@@@@@@@@################MM##@@@@@@##@@@@######@@@@@@MM####################              \n")
		escreva("                  ########################################################@@@@##########@@@@@@@@@@@@@@MM@@####################              \n")
		escreva("                ############################################@@##########@@@@@@@@MM@@@@@@@@@@@@@@@@@@MM@@@@@@####################            \n")
		escreva("              ##########################################################@@MMmmmmMMmmMMMMMM@@MMmmmmMMMMmmmmmm####################++          \n")
		escreva("              ######..mm######@@MMMMMM++::..--@@MM..++MMMMmm..----::++############################################################          \n")
		escreva("                ####@@##..------------------------########..--------mm####MMmm++--MM@@######@@##MM::@@####@@##@@--..++--##                  \n")
		escreva("                ####::++..----..----..------------########----------++##    --------------..####::----------------------##                  \n")
		escreva("                --##        ..######..                          ----::##@@####::          ########--          --##########                  \n")
		escreva("                ..##++    @@##@@##@@##            ..        mmmm----::##++++@@::        ##@@  ##  ####        --##########                  \n")
		escreva("                --##::    ##MM@@##@@MMMM        ####        ####..----########        ####    ##    ####      --##########                  \n")
		escreva("                --##::    ##@@@@##@@@@##        ####        ####------##            ::@@      ##      ####    --------::##..                \n")
		escreva("                --##      ##@@@@##@@@@##        ####        ####------####          ##        ##      MM####  ----MM######--                \n")
		escreva("                ..##      ##@@@@##@@@@##        ####        MM##------::++        ##          ##        MM####----########..                \n")
		escreva("                  ######  @@@@@@##@@@@##                        ----####MM      ##            ##          mm####--########                  \n")
		escreva("                  ######  @@@@@@##@@MM##                        @@####MM      ##              ##              ####--++..##                  \n")
		escreva("                ..##      @@MM@@##MM@@##                        ::######    ##                ##              ..##::--..##                  \n")
		escreva("                ..##@@##################################################::##                  ##              ----##++..##                  \n")
		escreva("                    --MM####@@@@@@@@##MMMMMMMMMM        MMMM          @@##                    ##              ..----######                  \n")
		escreva("                                                                      ############################::######################                  \n")
		escreva("                                                                                --##MM                  ##..        ######                  \n")
			}
			
	funcao estante(){
		
		escreva("        _________________________________________________________","\n")
		escreva("       ||-------------------------------------------------------||","\n")
		escreva("       ||.--.    .-._                        .----.             ||","\n")
		escreva("       |||==|____| |H|___            .---.___|\"\"\"\"|_____.--.___ ||","\n")
		escreva("       |||  |====| | |xxx|_          |+++|=-=|_  _|-=+=-|==|---|||","\n")
		escreva("       |||==|    | | |   | \\         |   |   |_\\/_|Reino|  | ^ |||","\n")
		escreva("       |||  |    | | |   |\\ \\   .--. |   |=-=|_/\\_|-=+=-|  | ^ |||","\n")
		escreva("       |||  |    | | |   |_\\ \\_( oo )|   |   |    |Fabul|  | ^ |||","\n")
		escreva("       |||==|====| |H|xxx|  \\ \\ |''| |+++|=-=|\"\"\"\"|-=+=-|==|---|||","\n")
		escreva("       ||`--^----'-^-^---'   `-' \"\"  '---^---^----^-----^--^---^||","\n")
		escreva("       ||-------------------------------------------------------||","\n")
		escreva("       ||-------------------------------------------------------||","\n")
		escreva("       ||               ___                   .-.__.-----. .---.||","\n")
		escreva("       ||              |===| .---.   __   .---| |XX|<(*)>|_|^^^|||","\n")
		escreva("       ||         ,  /(|   |_|III|__|''|__|:x:|=|  |     |=| Q |||","\n")
		escreva("       ||      _a'{ / (|===|+|   |++|  |==|   | |  |Story| | R |||","\n")
		escreva("       ||       '/\\/ _(|===|-|   |  |''|  |:x:|=|  |ReiXV| | Y |||","\n")
		escreva("       ||_____  -\\{___(|   |-|   |  |  |  |   | |  |     | | Z |||","\n")
		escreva("       ||       _(____)|===|+|[I]|DK|''|==|:x:|=|XX|<(*)>|=|^^^|||","\n")
		escreva("       ||              `---^-^---^--^--'--^---^-^--^-----^-^---^||","\n")
		escreva("       ||-------------------------------------------------------||","\n")
		escreva("       ||_______________________________________________________||","\n")
	}
	
	funcao masmorraIm(){
		
		escreva(" ---.----.__..----.----| _|_||___||___||___||___||___||___||_|_ |","\n")
		escreva("    |        |    |    | -.-..---..---..---..---..---..---..-.- |--.-","\n")
		escreva(" ---'--.-----'----'--.-|  | ||   ||   ||   ||   ||   ||   || |  | `|","\n")
		escreva("       |:           (| |  | ||   ||   ||   ||   ||   ||   || |  |--'-","\n")
		escreva("       |:.           | | _|_||___||___||___||___||___||___||_|_ |","\n")
		escreva(" ------'----.-.,----.'-| -.-..---..---..---..---..---..---..-.- |-.--","\n")
		escreva("        ,/) |       |  |  | ||   ||   ||   ||   ||   ||   || |  | |`","\n")
		escreva(" ----.---8--'--.----'--|  | ||   ||   ||   ||   ||   ||   || |  | |","\n")
		escreva("     |   8     |:      | _|_||___||___||___||___||___||___||_|_ |-'--","\n")
		escreva("     | ,)//    |:.     | -.-..---..---..---..---..---..---..-.- |:.","\n")
		escreva(" ----'-`=;'--.-'-.----.|  | ||   ||   ||   ||   ||   ||   || |  |--.-","\n")
		escreva("       //   /_ _( \\    |  | ||   ||   ||   ||   ||   ||   || |  | /|","\n")
		escreva(" ---.-//----)/\\,'_/----| _|_||___||___||___||___||___||___||_|_ | `|","\n")
		escreva("    |/|     `;=.(      | -.-..---..---..---..---..---..---..-.- |--'-","\n")
		escreva(" (  |`.`.   |`,-/      |,-'-||---||---||---||---||---||---||-'-.|","\n")
		escreva(" -`-'-.`.`-.';'=`.-..--'-.--------.-------------.--.-------.----'--.-","\n")
		escreva("      |  `-./.}{-'\\.)    |        )             |   `)     |       \\","\n")
		escreva("      |    :`-}{-''||    |:.      |   ,_        |          |:.     |","\n")
		escreva(" ---'`'-.--|`-}{-'||)----'-.------'--'.,`--.----'--------.-'-------'-","\n")
		escreva("        |  :`-`'-'/)|      |               |:.           |","\n")
		escreva(" -.-----'--;`.}{,`.||----,-'--------.------'---.--------,'--.,-------","\n")
		escreva("  |:     ,'/.`..'_(/(    |:         |          |             \\","\n")
		escreva("  |:.  ,',' |`--`.('))   |:.        |          |             |:","\n")
		escreva(" -'--,' <.._|__,. >`,----'----------'--------.,'-------------'----SSt","\n")
		escreva("     ``----....(','","\n")
		escreva("            _,'>'","\n")
		escreva("            )/","\n")
			}
	
}// fim do programa
