	funcao loja(){
		logico continuar = verdadeiro
		inteiro contador = 0
		escreve("A: Você gostaria de mais alguma coisa?\n")
		
		enquanto(continuar == verdadeiro) {
			
			escreve("[1] - É claro\n")
			escreve("[2] - Não no momento.\n")
			inteiro opcao
			leia(opcao)
			
			escolha(opcao)
			{
				caso 1:
					enquanto(continuar == verdadeiro){
					limpa()
					escreva("================================================================\n")
					escreva("|                       Seu Saldo:   "+  dinheiro + "D                      " )
					escreva("|                                                              |\n")
					escreva("|                                                              |\n")
					escreva("[ 1 ]       Pão          |  Recupera um pouco de vida  |  45G  |\n")
					escreva("[ 2 ]      Elixir        |  Um elixir quase milagroso  |  320G |\n")
					escreva("[ 3 ]  Pólvora explosiva |          Explode            |  250G |\n")
					escreva("[ 4 ]     Bandagens      |  Cuidam dos seus ferimentos |  100G |\n")
					escreva("[ 5 ]     Manoplas       | Vai doer mais quando bater  |  300G |\n")
					escreva("[ 0 ]                    |            Sair             |       |     \n")
					escreva("================================================================\n")
					leia(opcao)
					
					escolha(opcao)
					{
						caso 1:
							contador++
							
							se(dinheiro < 320){
								escreve("Você não tem dinheiro o suficiente para comprar isso.")
							} senao se(contador > 3){
								escreve("Você não tem espaço suficiente no inventário para comprar isso.")
							} senao {
								dinheiro -= 320
								escreve("Muito obrigado pela compra!")
								itens[2+contador] = "Elixir"
								checarItens()
								enter()
							}
							pare
	
						caso 2:
							
							contador++
							se(dinheiro < 45){
								escreve("Você não tem dinheiro o suficiente para comprar isso.")
							} senao se(contador > 3){
								escreve("Você não tem espaço suficiente no inventário para comprar isso.")
							} senao {
								dinheiro -= 45
								escreve("Muito obrigado pela compra!")
								itens[2+contador] = "Pão"
								checarItens()
								enter()
							}
							pare
	
						caso 3:
							contador++
							se(dinheiro < 250){
								escreve("Você não tem dinheiro o suficiente para comprar isso.")
							} senao se(contador > 3){
								escreve("Você não tem espaço suficiente no inventário para comprar isso.")
							} senao {
								dinheiro -= 250
								escreve("Muito obrigado pela compra!")
								itens[2+contador] = "Pólvora explosiva"
								checarItens()
								enter()
							}
							pare
	
						caso 4:
							contador++
							se(dinheiro < 100){
								escreve("Você não tem dinheiro o suficiente para comprar isso.")
							} senao se(contador > 3){
								escreve("Você não tem espaço suficiente no inventário para comprar isso.")
							} senao {
								dinheiro -= 100
								escreve("Muito obrigado pela compra!")
								itens[2+contador] = "Bandagens"
								checarItens()
								enter()
							}
							pare
	
						caso 5:
							contador++
							se(dinheiro < 300){
								escreve("Você não tem dinheiro o suficiente para comprar isso.")
							} senao se(contador > 3){
								escreve("Você não tem espaço suficiente no inventário para comprar isso.")
							} senao {
								dinheiro -= 300
								escreve("Muito obrigado pela compra!")
								itens[2+contador] = "Manoplas"
								checarItens()
								enter()
							}
							pare
	
						caso 0:
							escreve("Até logo então.")
							continuar = falso
							pare
		
							caso contrario:
							escreve("Essa opção é invalida. Escolha uma das opções: \n")
							pare
						
						
						}
					}
					
					continuar = falso
					pare

				caso 2:
					escreve("Ela te olha com desgosto.\n")
					continuar = falso
					pare

				caso contrario:
					escreve("Essa opção é invalida. Escolha uma das opções: \n")
					pare
			}//fim mais alguma coisa
			
		}
	}
	
