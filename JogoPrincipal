	funcao CasaCs1(){

		escreve("Você começa a preparar sua mochila.\n")
		escreve("Precisa organizar tudo para sua missão.\n")
		
		se(itens[2] == "faca"){
			escreve("Você já esta com sua faca. Não precisa de outra arma.\n")
		} senao {
			escreve("Você precisa de uma arma.\n")
			escreve("Você pega sua melhor faca.\n")
			Faca = verdadeiro
			escreve("Agora sim.\n")
		}

		escreve("Você não pode levar muitas coisas.\n")
		escreve("Caso alguma coisa vier te atacar, você tem que ser agil.\n")
		escreve("Uma mochila pesada irá te atrapalhar.\n")
		escreve("Você pega sua tocha, e pode levar mais alguma coisa.\n")
		itens[3] = "tocha"
		escreve("É sempre bom estar prevenido.\n\n")
		escreve("Qual item você quer levar?\n")

		logico continuar = verdadeiro
		enquanto(continuar == verdadeiro){
			
			escreve("[1] - Poção de Força\n")
			escreve("[2] - Poção de Cura \n")
			escreve("[3] - Poção de Resistência\n")
			inteiro opcao
			leia(opcao)

			escolha(opcao){
				caso 1: 
				itens[4] = "Poção de Força"
				escreve("Você coloca a poção de força na sua mochila.\n")
				pocaoForca = verdadeiro
				continuar = falso
				pare

				caso 2: 
				itens[4] = "Poção de Cura"
				escreve("Você coloca a poção de cura na sua mochila.\n")
				pocaoCura = verdadeiro
				continuar = falso
				pare

				caso 3: 
				itens[4] = "Poção de Resistência"
				escreve("Você coloca a poção de resistência na sua mochila.\n")
				pocaoResistencia = verdadeiro
				continuar = falso
				pare

				caso contrario:
				escreve("Essa opção é invalida. Escolha uma das opções: \n")
				
			}// fim do escolha
		}// fim do enquanto

		escreve("Agora sim, tudo pronto.\n")
		enter()
	
	}// fim da função

	funcao FlorestaSangrentaCs1(){
		
		escreve("Você sai de sua casa em rumo aos portões do reino.\n")
		escreve("Já está entardecendo, logo ficará de noite.\n")
		escreve("Melhor apressar o passo.\n")
		escreve("Você chega aos imensos portões do reino.\n")
		escreve("Guarda: Para onde esta indo senhor?\n")
		escreve("Você apenas mostra o passe para o guarda e ele se cala.\n")
		escreve("Tudo certo, rumo a floresta sangrenta.\n\n")
		escreve("A floresta não fica longe do reino, cerca de duas horas caminhando.\n")
		escreve("Seu corpo pede algo para comer e beber.\n")
		retc(600)
		escreve("Você finalmente chega.\n")
		FlorestaIm()
		enterCarta()

		escreve("A floresta é imensa e assombrosa.\n")
		escreve("Mesmo com todos seus anos de serviço, essa é sua primeira vez em território desconhecido.\n")
		escreve("Você precisa tomar muito cuidado.\n")
		escreve("- Onde a donzela pode estar?\n")
		escreve("Vai ser dificil encontra-la, principalmente a noite.\n")
		escreve("Você começa a adentrar a floresta.\n")
		escreve("O que deve ser feito primeiro?\n")

		logico continuar = verdadeiro
		enquanto(continuar == verdadeiro){
			
			escreve("[1] - Procurar comida\n")
			escreve("[2] - Procurar abrigo\n")
			inteiro opcao
			leia(opcao)

			escolha(opcao){
				caso 1: 
				escreve("Você continua andando para dentro da floresta.\n")
				escreve("Nenhum sinal de animais evidentes.\n")
				escreve("Está tudo um completo silencio.\n")
				retc(600)
				escreve("Após uns 10 minutos caminhando, você escuta um riacho.\n")
				escreve("Você segue em direção ao som e o encontra.\n")
				escreve("Rapidamente você se agacha e verifica se a água é doce.\n")
				escreve("Graças a Deus sim.\n")
				escreve("Você toma bastante água e tenta pegar algum peixe.\n")
				retc(500)
				escreve("Tudo funciona como o plenajedo.\n")
				escreve("Agora você volta para floresta para fazer um abrigo.\n")
				continuar = falso
				pare

				caso 2:
				escreve("Você continua andando para dentro da floresta.\n")
				escreve("Encontra um lugar supostamente seguro e amplo para fazer o abrigo.\n")
				continuar = falso
				pare
				

				caso contrario:
				escreve("Essa opção é invalida. Escolha uma das opções:\n")
				
			}// fim do escolha
		} // fim do enquanto

		escreve("Você procura folhas e começa a organiza-las como uma cama.\n")
		escreve("Não ascende nenhuma fogueira para não chamar predadores.\n")
		escreve("Não tem como saber o que existe aqui.\n")
		retc(600)
		escreve("Você cochila de forma suave.\n")
		escreve("Escutando tudo ao seu redor.\n")
		escreve("Você escuta galhos se quebrando perto de você.\n")
		escreve("Muito estranho. O que deve ser feito?\n")

		continuar = verdadeiro
		enquanto(continuar == verdadeiro){

			escreve("[1] - Continuar dormindo.\n")
			escreve("[2] - Ir investigar o barulho.\n")
			inteiro opcao
			leia(opcao)

			escolha(opcao){
				
				caso 1:
				escreve("Você ignora completamente o barulho.\n")
				escreve("Se vira de lado e tenta cochilar novamente.\n")
				escreve("Você sente o barulho se aproximando.\n")
				escreve("Quando levanta a cabeça e olha para o lado...\n")
				escreve("Você vê um enorme Ubariano.\n")
				escreve("E não um qualquer.\n")
				escreve("Um Ubariano da realeza.\n")
				Ubariano2()
				GameOver()
				continuar = falso
				gameLoop = falso
				pare

				caso 2:
				escreve("Você levanta devagar, sem fazer barulho.\n")
				escreve("Rapidamente pega sua mochila.\n")
				escreve("Você apunhala sua faca por precaução.\n")
				escreve("Devagar você anda em direção ao barulho.\n")
				escreve("Adentrando os arbustos, entre duas árvores...\n")
				escreve("você o ve. Um enorme Ubariano.\n")
				escreve("E não um qualquer, um da realeza.\n")
				Ubariano2()
				escreve("Ele é enorme. E está te encarando.\n")
				continuar = falso
				LutaUbariano = verdadeiro
				pare

				caso contrario:
				escreve("Essa opção é invalida. Escolha uma das opções: \n")
			}// fim do escolha
			
		}// fim do enquanto	
		
		enter()

		
	}// fim da funcao

	funcao LutaCs(inteiro cs){

		escreve("Voce rapidamente pega sua adaga. \n")
		escreve("Ele começa a se aproximar.\n")
		
		inteiro contador = 1
		logico continuar = verdadeiro

		enquanto(continuar == verdadeiro){

			se(vida > 100){
				vida = 100
			}
			
			se(cs == 1){
				Ubariano2()
				escreve("Vida do Ubariano: " + vidaUbariano2+ "\n" )	
			} senao se(cs == 2){
			Azlond()
			escreve("Vida do Azlond: " + vidaUbariano2+ "\n" )
			} senao se(cs == 3){
				Ubariano()
				escreve("Vida da criatura: " + vidaUbariano2+ "\n" )
			}
			escreve("Sua vida: " + vida + "\n\n")
			
			
			
			escreve("O que você vai fazer? \n")

			escreve("[1] - Atacar. \n")
			escreve("[2] - Se defender.\n")
			escreve("[3] - Tentar fugir.\n")
			escreve("[4] - Checar itens.\n")

			se(livro == verdadeiro){
				escreve("[5] - Usar Poção.\n")
			}

			dano = 0
			danoInimigo = 0
			inteiro opcao
			leia(opcao)
			cadeia frasesAtaque[3] = {"boca do estomâgo!", "costela!", "perna!"} // Fiz para gerarmos frases aleatorias, essa por exemplo, é de onde o caçador golpeou o ubariano.
			cadeia frasesAtaque2[3] = {"no quadril!", "na coxa!", "no peito!"}
			
			escolha(opcao){

				caso 1: 
				
				se(contador == 1){
					
					escreve("Você o golpeia na " + frasesAtaque[u.sorteia(0,2)] + "\n")
					escreve("Isso já te dá vantagem.\n")
					vidaUbariano2 -= u.sorteia(10, 20) + dano
					escreve("Vida do Ubariano: " + vidaUbariano2 + "\n\n" )
	
					escreve("Ele parece não ter gostado nada disso.\n")
					escreve("Ele corre mais rápido ainda em sua direção.\n")
					escreve("Ele tenta cravar as garras perto de suas pernas.\n")
					escreve("Você desvia do golpe, mas acaba caindo em pedras\n")
					vida -= u.sorteia(5,7) + danoInimigo
					escreve("Você se levanta rapidamente, apenas com alguns ralados.\n")
					se(cs == 1){
						escreve("O ubariano te olha agressivamente, e levanta voo.\n")
					} senao se(cs == 2){
						escreve("Afinal quem é esse cara? Não se parece como um humano comum.\n")
					}
							
				}// fim do se

				senao se(contador == 2){

					se(cs == 1){
					escreve("Você corre para o ubariano não pousar em você.\n")	
					}
					
					escreve("Ele vem em sua direção e tenta te agarrar.\n")
					escreve("Você crava a sua adaga "+ frasesAtaque2[u.sorteia(0,2)] + "\n\n")
					escreve("O sangue dele é azul. Ele urge de dor.\n")
					vidaUbariano2 -= u.sorteia(15, 23) + dano
					escreve("Você tenta correr dele, mas ele consegue te dar um chute!\n")
					escreve("Você cai no chão.\n")
					vida -= u.sorteia(7, 14) + danoInimigo
					escreve("Parece que ele está vindo pra cima de você novamente.\n")
					
				}// fim do se contador 2

				senao se(contador == 3){
					escreve("Você tenta surpreende-lo, levanta super rapido.\n")
					escreve("Corre na direção dele, desvia para a esquerda e vai para trás dele.\n")
					escreve("Antes que ele consiga se virar, você pula em cima dele.\n")
					escreve("Crava a adaga perto do pescoço dele.\n")
					vidaUbariano2 -= u.sorteia(20, 30) + dano
					escreve("Ele grita e você corre para longe.\n")
					escreve("Parece ter doido bastante, ele cai no chão de dor.\n")
				}
				senao se(contador == 4){
					escreve("Sem pensar muito, você corre para cima dele.\n")
					escreve("Você tenta cravar a adaga nele.\n")
					escreve("Ele é muito agíl, mas você consegue enfia-la no braço dele.\n")
					vidaUbariano2 -= u.sorteia(20, 25) + dano
					escreve("Ele grita de dor, você se levanta e se afasta dele.\n")
					
				}
				senao se(contador == 5){
					escreve("Ele grita e corre para você.\n")
					escreve("Você da uma rasteira nele, ele cai na hora.\n")
					vidaUbariano2 -= u.sorteia(7, 15) + dano
					escreve("Em contrapartida, ele arranha sua perna deixando quase em carne viva.\n")
					vida -= u.sorteia(10, 15) + danoInimigo
					escreve("Você se levanta, com muita dor.")
				}
				senao se(contador == 6){
					continuar = falso
				}
				pare
				// FIM DO CASO 1
				caso 2: 

				se(cs == 1){
				se (contador == 1){
					escreve("Você pega um pedaço de tronco do chão, e se defende das garras do ubariano.\n")
					escreve("Ele parece não ter gostado da sua ousadia.\n")
					escreve("Ele golpeia seu galho e o quebra em pedaços.\n")
					escreve("Você tenta procurar outra coisa para se defender.\n")
					escreve("Ao desviar sua atenção, o ubariano te da um chute na " + frasesAtaque[u.sorteia(0,2)] + "\n")
					vida -= u.sorteia(10,13) + danoInimigo
	
					escreve("A dor é terrível.\n")
					escreve("Você levanta rapidamente e o ubariano começa a levantar voo.\n")
				}// fim do se contador 1

				senao se(contador == 2){
					escreve("Ele vem voando em sua direção.\n")
					escreve("Você pega pedras e do chão e atira nele para se defender.\n")
					escreve("Uma das pedras pega nas asas dele com força e ele cai no chão.\n")
					vidaUbariano2 -= u.sorteia(20, 27) + dano
					escreve("Você tenta ir para mais longe dele, mas ele se arrasta pelo chão e te derruba.\n")
					escreve("Você cai em um arbusto cheio de espinhos dolorosos.\n")
					vida -= u.sorteia(10, 15) + danoInimigo
					escreve("Ele se levanta e parece estar vindo para cima de você novamente.\n")
				}// fim do se contador 2		
				senao se(contador == 4){
					escreve("Você se prepara, pega uma enorme casca de árvore perto de você.\n")
					escreve("O ubariano se levante e vem como se fosse lhe agarrar.\n")
					escreve("Você tenta se proteger com seu escudo improvisado.\n")
					escreve("Com uma unica mão ele quebra o escudo e acerta sua barriga.\n")
					vida -= u.sorteia(10, 15) + danoInimigo
					escreve("Com um dos pedaços quebrados de seu escudo, você crava-o na barriga dele.\n")
					vidaUbariano2 -= u.sorteia(10, 20) + dano
					escreve("Ele grita de dor e você se afasta.\n")
				}
				senao se(contador == 5){
					escreve("Você percebe a fúria no olhar dele.\n")
					escreve("Ele vem em sua direção, você esquiva e sai correndo.\n")
					escreve("Ele tenta voar, mas parece estar com as asas machucadas.\n")
					escreve("Você encontra uma pedra escalcante no chão. Ela é quente como o fogo.\n")
					escreve("Você a chuta na direção dele, criando uma queimadura.\n")
					vidaUbariano2 -= u.sorteia(20, 25) + dano
					escreve("Ao se debater pela dor, ele acaba te derrubando.\n")
					escreve("Você se levanta com muita dor.\n")
					
				}		
				}// fim do se cs == 1
				


				 se(contador == 3){
					escreve("Ele vem com as garras para cima de você.\n")
					escreve("Você dá um mortal para trás. Ele fica todo confuso.\n")
					escreve("Nunca deixe ele saber seu próximo passo.\n")
					escreve("Você pula e da um chute na garganta dele.Parece ter doido.\n")
					vidaUbariano2 -= u.sorteia(15, 23) + dano
					escreve("Ele bate em uma árvore atrás dele e cai.\n")
				}
				
				senao se(contador == 6){
					continuar = falso
				}
				pare
				//FIM DO CASO 2

				caso 3:
				se (contador == 1){
					
					escreve("Você olha para todos os lados procurando uma rota de fuga.\n")
					escreve("Você avisa um tronco enorme no chão, e corre para se esconder dentro dele.\n")
					escreve("Obviamente o ele viu exatamente para onde foi.\n")
					escreve("Ele pega o tronco com as mãos e te chacoalha, até você cair.\n")
					escreve("Seu pé é torcido ao cair.\n")
					vida -= u.sorteia(7, 10) + danoInimigo
					escreve("Você levanta com dificuldade.\n")
					se(cs == 1){
					escreve("O ubariano fica cada vez mais feroz e começa a levantar voo.\n")	
					}
					
				
				}// fim do se contador 1
				
				senao se (contador == 2){
					escreve("Ele vem em sua direção e você corre desesperadamente.\n")
					escreve("Você corre olhando para trás para não perde-lo de vista.\n")
					escreve("Ao se virar, você dá de cara em uma árvore.\n")
					escreve("Você cai no chão com a força do impacto.\n")
					vida -= u.sorteia(10, 15) + danoInimigo
					escreve("Ele pousa e parece estar vindo em sua direção.\n")
				}// fim do se contador 2

				senao se(contador == 3){
					escreve("Você tenta rastejar para fugir dele.\n")
					escreve("Ele te pega pelo pé e te joga longe.\n")
					escreve("Isso doi muito.\n")
					vida -= u.sorteia(10, 20) + danoInimigo
					escreve("Você se levanta com difuldade e pega uma enorme pedra no chão.\n")
					escreve("Você atira e acerta a cabeça dele.\n")
					escreve("Ele fica meio tonto e cai no chão.\n")
					vidaUbariano2 -= u.sorteia(15, 25) + dano
				}// fim do se contador 3
				
				senao se(contador == 4){
					escreve("Aproveitando que ele está no chão, você foge para dentro das árvores.\n")
					escreve("Ele começa a farejar você. Com agilidade vocÊ se esconde.\n")
					escreve("Ele te encontra e te agarra.\n")
					escreve("Você pega sua adaga e enfia-a na mão da criatura.\n")
					vidaUbariano2 -= u.sorteia(15,20) + dano
					escreve("Ele começa a sangrar e você se afasta.\n")
				}

				senao se(contador == 5){
					escreve("Ele vem correndo para sua direção.\n")
					escreve("Você se esquiva e sai correndo.\n")
					escreve("Obviamente ele vem atrás de você.\n")
					escreve("No caminho você ve uma pedra obscura, ela é uma pedra muito acida, machuca ao encostar.\n")
					escreve("Você chuta nele para dar mais vantagem.\n")
					vidaUbariano2 -= u.sorteia(13,18) + dano
					escreve("Ele ao se debater, te derruba.\n")
					vida -= u.sorteia(10,15) + danoInimigo
					escreve("Você se levanta, com muita dor.\n")
				}
				senao se(contador == 6){
					continuar = falso
				}
				pare
				// FIM DO CASO 3
				caso 4:
				
				escreva("| ")
				para(inteiro i=0; i < 6; i++){
				escreva(i, "- ", itens[i], " | ")}
				escreve("\n")

				escreve("Quer usar um de seus itens?\n")
				
				logico continuar2 = verdadeiro
				enquanto(continuar2 == verdadeiro){

					escreve("[1] - Sim. \n")
					escreve("[2] - Não.\n")
					leia(opcao)
					escolha(opcao){
					caso 1:

					escreve("Digite o número do item que você quer utilizar.\n")
					inteiro item
					leia(item)

						escolha(item){
							caso 0:
							escreve("Você não pode usar esse item.\n")
							continuar2 = falso
							pare
	
							caso 1:
							escreve("Você não pode usar esse item.\n")
							continuar2 = falso
							pare
	
							caso 2:
							escreve("Você já está usando esse item.\n")
							continuar2 = falso
							pare
	
							caso 3:
							usarItens(1)
							continuar2 = falso
							contador -= 1
							pare
	
							caso 4:
							usarItens(2)
							continuar2 = falso
							contador -= 1
							pare
	
							caso 5:
							usarItens(3)
							continuar2 = falso
							contador -= 1
							pare
							
						}// fim escolha item
					pare

					caso 2:
					continuar2 = falso
					pare 

					caso contrario:
					escreve("Essa opção é invalida. Escolha uma das opções: \n")
					
					}// fim do escolha
				}// fim do enquanto
				
				// FIM DO CASO 4
				caso 5:
				
				se(livro== verdadeiro){
					
					se(pocaoForca == verdadeiro){
						escreve("Você ingere a poção rapidamente.\n")	
						dano += u.sorteia(7, 12)
						pocaoForca = falso
						contador -= 1
					}

					senao se(pocaoCura == verdadeiro){
						escreve("Você ingere a poção rapidamente.\n")	
						vida += u.sorteia(7, 15)
						pocaoCura = falso
						contador -= 1
					}

					senao se(pocaoResistencia == verdadeiro){
						escreve("Você ingere a poção rapidamente.\n")	
						danoInimigo -= u.sorteia(7, 12)
						contador -= 1
						pocaoResistencia = falso
					}

					senao {
						escreve("Você não possui mais poções.\n")
					}
				}// fim do se livro
				pare 
				// FIM DO CASO 5
				caso contrario:
				escreve("Essa opção é invalida. O Ubariano esta vindo até você. \n")
				
			}// fim do escolha

			contador++
			se(contador == 6){
					continuar = falso
				}
		}// fim do enquanto

		escreve("Você percebe que estão próximos de um penhasco.\n")
		escreve("Sem pensar muito, você apenas corre e joga o ubariano do penhasco.\n")
		retc(600)
		escreve("Ele cai e não move nenhum musculo.\n")
		escreve("- É melhor descer para averiguar.\n")
		enter()
		
	}// fim da funcao

	funcao FinalCs1(){
		escreve("Devagar você desce o penhasco.\n")
		escreve("Ao chegar lá me baixo, você percebe que o ubariano parece estar...\n")
		escreve("Se destransformando?\n")
		escreve("Suas asas somem, ele começa a diminuir de tamanho e ganhar cabelo.\n")
		escreve("Cabelos longos. Um vestido rasgado e branco. Pele clara\n")
		escreve("É uma mulher...\n")
		escreve("Você checa o pulso dela.\n")
		retc(500)
		escreve("Esta morta.\n")
		escreve("Ao segurar o pulso dela, você percebe uma pulseira, com um nome.\n")
		u.aguarde(500)
		escreve("- Stella.\n")
		escreve("A donzela.\n")
		escreve("Você pega o colar para conferir, e sim.\n")
		escreve("É mesmo ela, A donzela.\n")
		escreve("Como ela virou um Ubariano? Um ubariano da realeza?\n")
		escreve("- Meu deus, eu matei a donzela.\n")
		escreve("- A escolhida dos Deuses.\n")
		escreve("- A protegida do rei.\n")
		escreve("- Eu tenho que fugir. Agora.\n")
		escreve("Você rapidamente pega tudo, e segue em direção a Ubar.\n")
		escreve("Será o unico lugar que o rei não irá procurar.\n")
		fim()
		escreve("Final definitivo 1/3: Final ruim\n")
		gameLoop = falso
	}
	
	funcao MasmorraCs2(){
		escreve("- O que é isso?\n")
		escreve("Sua visão está muito embaçada, e sua cabeça lateja de dor.\n")
		masmorraIm()
		enterCarta()
		escreve("Você está em uma masmorra.\n")
		escreve("Com janelas que dão pra ver apenas outras masmorras.\n")
		escreve("Está muito frio e nemhum sinal de vento.\n")
		escreve("Você deve estar no subsolo.\n")
		escreve("Sem nenhum sinal de escapatória evidente.\n")
		escreve("Pelo menos você está solto, sem cordas amarrando você.\n")
		escreve("Você anda de um lado para o outro, pensativo.\n")
		escreve("As horas estão passando e eu tenho cada vez menos tempo.\n")
		escreve("Você verifica e percebe que sua faca ainda está com você.\n")
		escreve("Felizmente, anos de experiência te deram a habilidade de esconder armas como ninguém.\n")
		escreve("Você serra as grades e remove-as com muito cuidado.\n")
		escreve("Hora de sair daqui.\n")
		u.aguarde(300)
		escreve("No entanto")
		retc(200)
		batalhaEsqueleto()
		
		
	}

	funcao logico SaindoMasmorraCs2(){
		
		escreve("Como pode um homem morto voltar à vida assim?")
		escreve("Assustador mas, de qualquer forma, hora de ir.\n")
		escreve("Você caminha por grandes corredores até se deparar com um guarda cuidando a saída.\n")
		escreve("Ele está de costas e não te viu, o que fazer?\n")
		
		
		logico continuar = verdadeiro
		enquanto(continuar == verdadeiro){

			escreve("[1] - Atacar furtivamente\n")
			escreve("[2] - Gritar e ir pra cima\n")
			inteiro opcao
			leia(opcao)
		
			escolha(opcao){
				caso 1:
					escreve("Você se aproxima furtivamente e o golpeia sem ele perceber.\n")
					escreve("- Me assusta o quão conveniente fácil foi escapar daqui.\n")
					escreve("Você então abre a porta e sai pra fora.")
					enter()
					limpa()
					continuar = falso
					florestaCs2 = verdadeiro
					pare

				caso 2:
					escreve("Você dá um grito intimidador e vai pra cima do guarda com tudo.\n")
					escreve("Antes que você conseguisse acertar qualquer golpe nele, ele crava a espada em você.\n")
					retc(300)
					escreve("Não foi uma boa ideia.")
					limpa()
					GameOver()
					escreve("Dica: Na maioria das vezes a solução mais idiota é a pior.")
					continuar = falso
					gameLoop = falso
					pare

				caso contrario: 
					escreve("Essa opção é invalida. Escolha uma das opções: \n")
			
			}// fim do escolha
		}// fim do enquanto

		retorne florestaCs2
		
	}// fim da função

	funcao florestaSangrentaCs2(){
		FlorestaIm()
		escreve("\nParece que você está em uma floresta.\n")
		escreve("Ao que parece, está de noite, e você não faz a menor ideia de onde está.\n")
		escreve("Isso é péssimo.")
		retc(400)
		enter()
		limpa()
		escreve("\nVocê consegue ouvir barulhos. Seguindo a direção deles você encontra uma menina amarrada.\n")
		escreve("O que você vai fazer?\n")
		logico continuar = verdadeiro
		enquanto(continuar == verdadeiro){
			escreve("[1] - Checar.\n")
			escreve("[2] - Deixar quieto.\n")
			inteiro opcao
			leia(opcao)

			escolha(opcao){
				caso 1:
					escreve("Você vai até ela e começa a desamarrá-la.\n")
					escreve("Ela parece inconsciente.\n")
					escreve("Ao olhar para trás você percebe a atendente e um homem estranho.\n")
					escreve("?: Você está se metendo em algo muito grande sabia?\n")
					escreve("?: Meu nome é Azlond e eu não deixarei você levar nossa garantia.\n")
					escreve("A: Agora, te dou a chance de ir embora e viver a sua vida, onde quer que seja.\n")
						enquanto(continuar == verdadeiro){
							escreve("[1] - Ir embora.\n")
							escreve("[2] - Ficar.\n")
							leia(opcao)
								escolha(opcao){
									caso 1:
										escreve("Você vira as costas pra ir embora mas é atingido nas costas.")
										limpa()
										GameOver()
										escreve("\nFinal x/x: Nunca confie numa pessoa armada")
										continuar = falso
										gameLoop = falso
									pare

									caso 2:
										escreve("- Não vou sem a garota.\n")
										escreve("A: Entendo, então você é muito imprudente.\n")
										escreve("- O que vocês querem com ela?\n")
										escreve("A: Ela é a chave para a nossa vitória, nossa liberdade. Seremos um povo superior com ela.\n")
										escreve("A: Mas antes, preciso dar um jeito em você.")
										escreve("Ele tira o capacete e vem em sua direção.\n")
										continuar = falso
									pare

									caso contrario:
										escreve("Essa opção é inválida. Escolha uma das opções:\n")

									

								}//fim do escolha
						}//fim do enquanto ir embora
						
				continuar = falso
				pare

				caso 2:
					escreve("Você decide deixar ela lá e seguir seu caminho.\n")
					escreve("Você segue em frente mas")
					retc(300)
					escreve("Os barulhos que você escuta em seguida ficarão marcados em sua memória.\n")
					retc(800)
					escreve("De repente, você sente um mau presságio. Talvez aquela menina amarrada fosse a própria donzela.\n")
					escreve("Pelo que você ouviu na floresta mais cedo, ela já não está mais viva. ")
					limpa()
					GameOver()
					continuar = falso
					gameLoop = falso
				pare

				caso contrario:
					escreve("Essa opção é inválida. Escolha uma das opções:\n")
				pare
				
			} //Fim do escolha checar
		}// fim do enquanto 
	}// fim da função

	funcao FinalCs2(){
		retc(450)
		escreve("Quando você chega lá, ele sumiu. Já está quase amanhecendo e ele desapareceu.\n")
		escreve("Isso com certeza é preocupante, mas agora sua única preocupação agora é a donzela.\n")
		escreve("Você não sabe se ele morreu ou se vai voltar, mas vai até a garota para levá-la ao rei.\n")
		escreve("Você coloca o colar que achou mais cedo nela e checa seus batimentos. Ela parece bem, mas não quer acordar.\n")
		escreve("Você olha pro seu estado e vê o quanto está acabado.\n")
		escreve("- Vou cobrar o dobro do pagamento dessa vez.\n")
		fim()
		escreve("Final definitivo 2/3: Final bom")
		gameLoop = falso
		
	}

	funcao investigacaoCs3(){
		escreve("- Definitivamente tem algo estranho com essa taverna.\n")
		escreve("- Eles sabem de alguma coisa. E pior, sabem que eu sei de alguma coisa. \n")
		escreve("- Talvez eles até venham atrás de mim. O que seria mais prudente eu fazer?\n")
		escreve("- Além do mais, eu não tenho nenhuma pista de onde a suposta donzela está.\n")
		escreve("???: Parece que você está sem sorte também hahah. Você fez o que? Deu em cima da menina de olhos verdes?\n")
		escreve("Parece que um homem jogado na rua está falando com você.\n")
		escreve("- Do que você está falando?\n")
		escreve("???: Sabe outro dia eu estava ali dentro, bebendo, me divertindo e jogando meu charme para as mulheres.\n")
		escreve("???: Eu estava sem sorte aquele dia. Mas minha sorte mudou quando ela chegou.\n")
		escreve("???: Uma linda menina de olhos verdes. Talvez a primeira que eu tenha visto. \n")
		escreve("???: Tentei usar meu charme com ela mas me expulsaram do bar.\n")
		escreve("???: Esperei ela sair de lá e segui ela até onde pude... Mas quando ela entrou na floresta eu achei melhor deixar quieto.\n")
		escreve("- Você está falando da \n")
		escreva("???: Floresta sangrenta.\n")
		escreve("???: E tem um motivo pra ela se chamar assim. Eu é que não arrisco.\n")
		escreve("Você é um anjo, essa só pode ser a donzela.\n")
		escreve("???: Agora")
		retc(400)
		escreve("Você pode me dar uma esmola?\n")
		escreve("- Não.")
		u.aguarde(300)
		escreve("E você não deveria seguir as mulheres assim. Toma jeito.\n")
		escreve("Agora, sem perder mais tempo, o melhor a se fazer é ir para floresta sangrenta.\n")
		enter()
		
	}

	funcao florestaSangrentaCs3(){
		limpa()
		FlorestaIm()
		escreve("\nParece uma floresta muito fechada. O que quer que a donzela tenha ido fazer aí, \n")
		escreve("não parece ter terminado bem. Você espera encontrá-la com vida.\n")
		escreve("Você então adentra a floresta.\n")
		u.aguarde(650)
		enter()
		escreve("Você caminha por algumas horas e não encontra nada.\n")
		escreve("Já está anoitecendo e não há nada em lugar nenhum.\n")
		escreve("Tem sons estranhos por todo canto, você começa a se arrepender de ter vindo.\n")
		escreve("Você ouve um barulho estranho atrás de você e, quando olha pra trás, se depara com isso te olhando pelas árvores\n")
		criatura()
		escreve("Você vira pra trás de volta e, discretamente, sai correndo na maior velocidade possível.\n")
		escreve("Você esbarra em diversas árvores e até perde seu senso de direção.\n")
		escreve("Quando você finalmente cansa e para pra avaliar onde está, você ouve mais barulhos estranhos.\n")
		escreve("Mas dessa vez, parecem de humanos.\n")
		escreve("Você segue as vozes. Parece que tem um homem e uma mulher conversando. Você avista uma cabana e duas pessoas.\n")
		escreve("Uma delas é uma jovem moça. Será que é a donzela? Você decide arriscar...\n")
		escreve("- Donzela??\n")
		escreve("Ela se assuta.\n")
		escreve("D: Quem está aí??")
		escreve("Antes que você pudesse responder, uma criatura vai pra cima de você.\n")
		
		
		
	}

	funcao FinalCs3(){
		escreve("Você quase não sobreviveu. Mas agora é hora de ir até a donzela.\n")
		escreve("Você vai até ela e ela ainda está lá.\n")
		escreve("Ela parece surpresa em te ver\n")
		escreve("D: Quem é você? O que você quer comigo? Como você está vivo?\n")
		escreve("- Donzela, o que você faz aqui? O rei está à sua procura.\n")
		escreve("D: Não me chame assim e eu não voltarei com você para esse maldito castelo.\n")
		escreve("D: Escuta só, não sei como você sobreviveu ao meu soldado, mas tem mais centenas desses nessa floresta.\n")
		escreve("- Seu soldado? Do que você está falando?\n")
		escreve("D: Esse reino é uma desgraça. Somos um povo oprimido há muito tempo. Eles nos rejeitaram.\n")
		escreve("D: A divina corte é igual à nós, mas ela tem todo o poder.\n")
		escreve("D: Eu fui enviada há mais de 20 anos pra cá, e chegou a hora de por um fim nisso.\n")
		escreve("D: Olha, eu não sei se um plebeu como você sabe algo sobre honra ou posição certa.\n")
		escreve("D: Mas você tem muito valor físico. Te dou uma escolha agora. Ou você se junta ao lado certo, ou morre com seu povo.\n ")
		logico continuar = verdadeiro
		enquanto(continuar == verdadeiro){
			escreve("[1] - Se juntar a eles.\n")
			escreve("[2] - Eu não me importo. Vou pra casa.\n")
			inteiro opcao
			leia(opcao)
			escolha(opcao){
				caso 1:
				escreve("D: Perfeito. Você está prestes a viver história.\n")
				fim()
				escreve("Final definitivo 3/3: Final verdadeiro.")
				continuar = falso
				gameLoop = falso
				pare

				caso 2:
				escreve("D: Que seja, a história começa aqui.\n")
				escreve("Num estalar de dedos dezenas daquelas criaturas voam pra cima de você.\n")
				GameOver()
				escreve("Um final digno e honroso")
				retc(400)
				escreve("Ou não.")
				continuar = falso
				gameLoop = falso
				pare

				caso contrario: 
				escreve("Essa opção é invalida. Escolha uma das opções: \n")
				
			}
		}
	}

	funcao fim(){
escreva(" ███████████  ███                 ","\n")
escreva("▒▒███▒▒▒▒▒▒█ ▒▒▒                  ","\n")
escreva(" ▒███   █ ▒  ████  █████████████  ","\n")
escreva(" ▒███████   ▒▒███ ▒▒███▒▒███▒▒███ ","\n")
escreva(" ▒███▒▒▒█    ▒███  ▒███ ▒███ ▒███ ","\n")
escreva(" ▒███  ▒     ▒███  ▒███ ▒███ ▒███ ","\n")
escreva(" █████       █████ █████▒███ █████","\n")
escreva("▒▒▒▒▒       ▒▒▒▒▒ ▒▒▒▒▒ ▒▒▒ ▒▒▒▒▒ ","\n")
	}
	
	funcao escreve(cadeia texto)
	
	{ //uma espécie de "escreva()" porém pausadamente. 
							
		caracter letra
		inteiro velocidade = 15 //pausa padrão entre um caracter e outro.
		
		para(inteiro i = 0 ; i < tx.numero_caracteres(texto) ; i++){ //repete enquanto a frase inteiro não for escrita.

			letra = tx.obter_caracter(texto, i) //obtem o caracter da frase.
			
			escreva(letra)

			u.aguarde(velocidade)
			
			escolha(letra){ //verifica diferentes tipos de caracter para efetuar (ou não) determinadas pausas.

				caso ' ': u.aguarde(0) pare				
				caso ',': u.aguarde(velocidade * 3) pare	
				caso ':': u.aguarde(velocidade * 3) pare	
				caso ';': u.aguarde(velocidade * 3) pare		
				caso '.': u.aguarde(velocidade * 5) pare			
				caso '!': u.aguarde(velocidade * 5) pare				
				caso '?': u.aguarde(velocidade * 6) pare				
				caso contrario: u.aguarde(velocidade) pare
				
			}// fim do escolha
		}// fim do para
	}// fim da funcao
	
	funcao enterCarta(){
		escreve("Pressione Enter para continuar.")
		cadeia next
		leia(next)
		escreva("\n")
	}
	
	funcao enter(){
		cadeia next
		leia(next)

		limpa()
	}
	
	funcao retc(inteiro velocidade) { //É uma função que faz esse efeito de ... mais devagar
		escreve(".")
		u.aguarde(velocidade)
		escreve(".")
		u.aguarde(velocidade)
		escreve(".")
		u.aguarde(velocidade)
		escreva("\n")
	}
	
