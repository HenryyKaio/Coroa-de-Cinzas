	funcao criatura(){
escreva(" .-._                                                   _,-,","\n")
escreva("  `._`-._                                           _,-'_,'","\n")
escreva("     `._ `-._                                   _,-' _,'","\n")
escreva("        `._  `-._        __.-----.__        _,-'  _,'","\n")
escreva("           `._   `#===\"\"\"           \"\"\"===#'   _,'","\n")
escreva("              `._/)  ._               _.  (\\_,'","\n")
escreva("               )*'     **.__     __.**     '*( ","\n")
escreva("               #  .==..__  \"\"   \"\"  __..==,  # ","\n")
escreva("              #   `\\\"._(_).       .(_)_.'\\\"'   #","\n")
escreva("\n")
	}

	funcao batalhaEsqueleto( ){
		
		logico continuar = verdadeiro
		
		enquanto(continuar == verdadeiro ){
			
			se(vida == 0){
				GameOver()
				escreve("Você morreu.")
				continuar = falso
				gameLoop = falso
				
			}
			escreva("Vida: ", vidaInimigo, "hp\n")
			escreva("                              _.--\"\"-._                     \n")
			escreva("  .                         .\"         \".                   \n")
			escreva(" / \\    ,^.         /(     Y             |      )\\          \n")
			escreva("/   `---. |--'\\    (  \\__..'--   -   -- -'\"\"-.-'  )         \n")
			escreva("|        :|    `>   '.     l_..-------.._l      .'          \n")
			escreva("|      __l;__ .'      \"-.__.||_.-'v'-._||`\"----\"            \n")
			escreva(" \\  .-' | |  `              l._       _.'                   \n")
			escreva("  \\/    | |                   l`^^'^^'j                     \n")
			escreva("        | |                _   \\_____/     _                \n")
			escreva("        j |               l `--__)-'(__.--' |               \n")
			escreva("        | |               | /`---``-----'\"1 |  ,-----.      \n")
			escreva("        | |               )/  `--' '---'   \\'-'  ___  `-.   \n")
			escreva("        | |              //  `-'  '`----'  /  ,-'   I`.  \\  \n")
			escreva("      _ L |_            //  `-.-.'`-----' /  /  |   |  `. \\ \n")
			escreva("     '._' / \\         _/(   `/   )- ---' ;  /__.J   L.__.\\ :\n")
			escreva("      `._;/7(-.......'  /        ) (     |  |            | |\n")
			escreva("      `._;l _'--------_/        )-'/     :  |___.    _._./ ;\n")
			escreva("        | |                 .__ )-'\\  __  \\  \\  I   1   / / \n")
			escreva("        `-'                /   `-\\-(-'   \\ \\  `.|   | ,' /  \n")
			escreva("                           \\__  `-'    __/  `-. `---'',-'   \n")
			escreva("                              )-._.-- (        `-----'      \n")
			escreva("                             )(  l\\ o ('..-.                \n")
			escreva("                       _..--' _'-' '--'.-. |                \n")
			escreva("                __,,-'' _,,-''            \\ \\               \n")
			escreva("               f'. _,,-'                   \\ \\              \n")
			escreva("              ()--  |                       \\ \\             \n")
			escreva("                \\.  |                       /  \\            \n")
			escreva("                  \\ \\                      |._  |           \n")
			escreva("                   \\ \\                     |  ()|           \n")
			escreva("                    \\ \\                     \\  /            \n")
			escreva("                     ) `-.                   | |            \n")
			escreva("                    // .__)                  | |            \n")
			escreva("                 _.//7'                      | |            \n")
			escreva("               '---'                         j_| `          \n")
			escreva("                                            (| |            \n")
			escreva("                                             |  \\           \n")
			escreva("                                             |lllj          \n")
			escreva("                                             |||||    \n")
			escreva("\n")
			checarVida()
		
		
		

			
				
				escreva("[1] - Tentar fugir\n")
				escreva("[2] - Atacar\n")
				escreva("[3] - Usar itens\n")
				escreva("[4] - Defender\n\n")
				
				inteiro opcao 
				leia(opcao)
				
				escolha(opcao){
					
				caso 1:
					escreve("Não tem pra onde fugir. É uma cela.")
					pare
						
					
				caso 2:
					inteiro desviar = u.sorteia(1, 12)
					
					
					se(desviar == 5){
						escreve("\nVocê não acertou o ataque.\n")
						escreve("Ele contraatacou.")
						vida -= dano
						u.aguarde(1000)
					}
					senao{
						escreve("\nVocê acerta um ataque nele.\n")
						vidaInimigo -= dano
						u.aguarde(1000)
						
					}
		
					pare
					
				caso 3:
					escreve("\nQual item você quer usar?\n")
					logico continuar2 = verdadeiro
					enquanto(continuar2 == verdadeiro){
						escreve("[1] " + itens[3] + "\n")
						escreve("[2] " + itens[4] + "\n")
						escreve("[3] " + itens[5] + "\n")
						escreve("[4] Deixa pra lá.\n")
						leia(opcao)
						escolha(opcao){
							caso 1:
								usarItens(1)
								continuar2 = falso
							pare

							caso 2:
								usarItens(2)
								continuar2 = falso
							pare

							caso 3:
								usarItens(3)
								continuar2 = falso
							pare
							
						}//fim do escolha itens
					}
					
					

					pare

				caso 4:
					
					escreve("Você bloqueia o próximo ataque do esqueleto.")
					inteiro percentual = u.sorteia(5,15)/100
					vida -= danoInimigo - (danoInimigo*percentual)
				
				}
				se(vidaInimigo <= 0){
					continuar = falso
					escreva("\nVocê venceu essa batalha.\n")
				}
				senao se(vida <= 0){
					continuar = falso
					escreva("\nVocê perdeu essa batalha.\n ")
				}
				
			}
	}

	funcao usarItens(inteiro slot){
		
		se(itens[2+slot] == "Pão"){
			escreve("Você comeu o pão. Agora se sente melhor.\n")
			vida += 20
			itens[2+slot] = "vazio"
		
		} senao se(itens[2+slot] == "Elixir"){
			escreve("Você usou o elixir. Você se sente muito muito bem.\n")
			se(vida > 80){
				vida = 130
			} senao {
				vida = 100
			}
			danoInimigo -= 5
			itens[2+slot] = "vazio"
			
		} senao se(itens[2+slot] == "Pólvora explosiva"){
			escreve("Você se afasta e usa a pólvora. Ela explodiu.\n")
			escreve("Como você estava mais longe, não ficou muito ferido.")
			vidaInimigo -= 30
			vidaUbariano2 -= vidaInimigo
			vida -= 30
			itens[2+slot] = "vazio"
			
		} senao se(itens[2+slot] == "Bandagens"){
			escreve("Você usa as bandagens.\n")
			vida += 10
			danoInimigo -=  2
			itens[2+slot] = "vazio"
		} senao se(itens[2+slot] == "Manoplas"){
			escreve("Você equipou as manoplas e agora pode bater mais forte.\n")
			dano = u.sorteia(5, 10) + 10
			itens[2+slot] = "vazio"
		} senao {
			escreve("Não há nada para usar.")
		}
	}

	funcao pagina110(){
		escreva("╭────────────────────────────────────────────╮", "\n")
		escreva("│                                            │", "\n")
		escreva("│  Querido diário,                           │", "\n")
		escreva("│  não sei mais o que fazer,                 │", "\n")
		escreva("│  minhas mudanças de humor estão            │", "\n")
		escreva("│  me matando. Não tenho apetite             │", "\n")
		escreva("│  para nada, isso esta me preocupando.      │", "\n")
		escreva("│  Vou ir para a floresta sangrenta          │", "\n")
		escreva("│  procurar a erva Tocrunda, os livros       │", "\n")
		escreva("│  dizem que cura tudo. Deve curar tudo      │", "\n")
		escreva("│  isso que estou sentindo também.           │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│                                            │", "\n")
		escreva("│  Página 110                                │", "\n")
		escreva("│                                            │", "\n")
		escreva("╰────────────────────────────────────────────╯", "\n")
		enterCarta()
	}
	
